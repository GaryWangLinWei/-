static string AddBinary(string a, string b)
        {
            int i = a.Length-1;
            int j = b.Length-1;
            int c = 0;
            string s = null;
            while (i >= 0 || j >= 0||c==1)
            {
                c += (i >= 0) ? a[i--] - '0' : 0;
                c += (j >= 0) ? b[j--] - '0' : 0;
                s = (c % 2).ToString() + s;
                c = c / 2;
            }
            return s;
        }
