        static int[,] MatrixReshape(int[,] nums, int r, int c)
        {
            int[,] newNums=new int[r,c];
            int m = nums.GetLength(0);
            int n = nums.GetLength(1);
            if (m*n!=r*c) return nums;
            for (int i = 0; i < r * c; i++)
            {
                newNums[i / c, i % c] = nums[i / n, i % n];
            }
            return newNums;
        }
